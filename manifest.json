{
  "name": "Getting Started Example: Background color",
  "description": "Build first Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  //Chrome action: https://developer.chrome.com/docs/extensions/reference/action/
  "action": {
    "default_popup": "popup.html",
    //icons for toolbar
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  // Create and register additional icons: https://developer.chrome.com/docs/extensions/mv3/user_interface/#icon_size
  // icons needed on management page and other places
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html"
}